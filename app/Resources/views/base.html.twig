<!DOCTYPE html>
<!-- Template by Quackit.com -->
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>Maps Warehouse</title>
        
{% block stylesheets %}
    {% stylesheets 'bundles/map/css/*' %}
    <link href="{{ asset('bundles/map/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/map/css/custom.css') }}" rel="stylesheet">
    {% endstylesheets %}
{% endblock %}
{% block javascripts %}
    {% javascripts %}
    <script src="{{ asset('bundles/map/js/jquery-1.11.3.min.min.js') }}"></script> 
    <script src="{{ asset('bundles/map/js/bootstrap.min.js') }}"></script> 
    <script src="{{ asset('bundles/map/js/bootstrap.js') }}"></script>
    <script src="{{ asset('bundles/map/js/holder.min.js') }}"></script>
    {% endjavascripts %}
{% endblock %}
      
</head>
<body>
{% block navbar %}
    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <!-- Logo and responsive toggle -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">
                	<span class="glyphicon glyphicon-globe"></span> 
                </a>
            </div>
            <!-- Navbar links -->
            <div class="collapse navbar-collapse" id="navbar">
                <ul class="nav navbar-nav">
                    <li class="active">
                        <a href="http://192.168.33.22:8080/user">Home</a>
                    </li>
                    <li>
                        <a href="http://192.168.33.22:8080/category/showAll">Kategorie</a>
                    </li>
                 </ul>

                                <!-- Login -->
                                {% block login %}
                                <form class="navbar-form navbar-right" role="search">
                                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                                    Witaj: {{ app.user.username }}
                                    <a href="http://192.168.33.22:8080/logout">Wyloguj</a>
                                {% else %}
					<div class="form-group">
                                        <a href="http://192.168.33.22:8080/login">Zaloguj</a>
                                {% endif %}
                                </form>
                                {% endblock %}

            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    {% endblock %}

    {% block body %}
	<div class="jumbotron feature">
		<div class="container">
			<h1>Znajdź swój kawałek świata</h1>
		</div>
	</div>

    <!-- Content -->
    <div class="container">

        <!-- Feature Row -->
        <div class="row">
            <article class="col-md-4 article-intro">
                    <img class="img-responsive img-rounded" src="bundles/map/thumbnails/98ea1a90e071d8b7a52d0d894288c134.jpeg" alt="">
                </a>
                <h3>
                    Mapy
                </h3>
                <p></p>
            </article>
            <article class="col-md-4 article-intro">
                <a href="#">
                    <img class="img-responsive img-rounded" src="bundles/map/thumbnails//3bad68f31eacefc9d194ca1347101487.jpeg" alt="">
                </a>
                <h3>
                    Geograficzne bazy danych
                </h3>
                <p></p>
            </article>

            <article class="col-md-4 article-intro">
                <a href="#">
                    <img class="img-responsive img-rounded" src="bundles/map/thumbnails//c733673d76846603b653af98c810c570.jpeg" alt="">
                </a>
                <h3>
                    Dane w formatach .shp
                </h3>
                <p></p>
            </article>
            
        </div>
        <!-- /.row -->

    </div>
    <!-- /.container -->
{% endblock %}	

{% block footer %}
<footer>
    <div class="small-print">
            <div class="container">
                    <p> <a href="#">Kontakt</a></p><p>Template from: &copy; Example.com </p>
            </div>
    </div>
</footer>   
    
{% endblock %}
    
